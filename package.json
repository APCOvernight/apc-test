{
  "name": "apc-test",
  "version": "1.0.4",
  "description": "APC Test stack bundled into one package",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && nyc mocha",
    "lint": "eslint *.js test/**/*.js src/**/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=text-lcov | codeclimate-test-reporter",
    "stryker": "stryker run"
  },
  "dependencies": {
    "chai": "4.1.2",
    "codeclimate-test-reporter": "0.5.0",
    "coveralls": "3.0.0",
    "mocha": "5.0.4",
    "sinon": "4.4.4",
    "nyc": "11.6.0",
    "sinon-chai": "3.0.0",
    "stryker": "0.19.1",
    "stryker-api": "0.13.0",
    "stryker-html-reporter": "0.12.1",
    "stryker-javascript-mutator": "0.4.0",
    "stryker-mocha-framework": "0.8.3",
    "stryker-mocha-runner": "0.10.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/APCOvernight/apc-test.git"
  },
  "keywords": [
    "test",
    "js",
    "testing",
    "mocha",
    "chai",
    "stryker",
    "node"
  ],
  "author": "Ian Egner <http://twitter.com/ianegner>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/APCOvernight/apc-test/issues"
  },
  "homepage": "https://github.com/APCOvernight/apc-test#readme",
  "devDependencies": {
    "apc-style": "1.1.0"
  }
}
