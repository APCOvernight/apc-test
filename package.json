{
  "name": "apc-test",
  "version": "1.2.0",
  "description": "APC Test stack bundled into one package",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && nyc mocha",
    "lint": "eslint *.js test/**/*.js src/**/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "stryker": "stryker run",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "chai": "4.2.0",
    "coveralls": "3.0.2",
    "mocha": "6.0.1",
    "nyc": "13.3.0",
    "sinon": "7.2.4",
    "sinon-chai": "3.3.0",
    "stryker": "0.35.1",
    "stryker-api": "0.24.1",
    "stryker-html-reporter": "0.18.1",
    "stryker-javascript-mutator": "0.14.1",
    "stryker-mocha-framework": "0.15.1",
    "stryker-mocha-runner": "0.17.1",
    "snyk": "^1.189.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/APCOvernight/apc-test.git"
  },
  "keywords": [
    "test",
    "js",
    "testing",
    "mocha",
    "chai",
    "stryker",
    "node"
  ],
  "author": "Ian Egner <http://twitter.com/ianegner>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/APCOvernight/apc-test/issues"
  },
  "homepage": "https://github.com/APCOvernight/apc-test#readme",
  "devDependencies": {
    "apc-style": "1.4.0"
  },
  "snyk": true
}
